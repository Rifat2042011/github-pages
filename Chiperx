  <script>
    const PASSWORD = "boss123";

    function unlockPages() {
      const input = document.getElementById('globalPasswordInput').value;
      if (input === PASSWORD) {
        document.getElementById('lockscreen').classList.add('hidden');
        document.getElementById('protectedPages').classList.remove('hidden');

        const saved = localStorage.getItem('cipherx-notes');
        if (saved) {
          document.getElementById('ideaBox').value = saved;
        }
      } else {
        alert("Wrong password, intruder detected!");
      }
    }

    function saveIdea() {
      const idea = document.getElementById('ideaBox').value;
      localStorage.setItem('cipherx-notes', idea);
      alert("Saved successfully!");
    } 
  </script>
